{{#extend "layout"}}
  {{#content "body"}}


    <section class="p-t-24">

      <div class="container">
        {{> breadcrumb}}
        {{> editorialHeader}}


        <div class="columns p-t-24">
          <div class="column is-8-tablet">
            {{> articleContent}}
          </div>
          <div class="column is-4 is-sidebar is-hidden-touch p-t-24">
            {{> sidebarList}}
            {{> solutions}}
            {{> sidebarNewsletter}}
            {{> sidebarList}}
            {{> socialSidebar}}
          </div>
          <div class="column is-hidden-desktop">
            {{> articleList}}
          </div>
        </div>


      </div>

      <br>

      <div class="is-hidden-desktop m-t-8">
        {{> editorialMobile}}
      </div>

      <div class="has-background-display-lighter p-t-32 p-b-32">
        <div class="container">
          {{> productsSlider}}
        </div>
      </div>
      {{> sidebarNewsletter large=true}}
      
      <div class="is-hidden-desktop">
        {{> contactInfo}}
      </div>
      
      
    </section>

  {{/content}}
{{/extend}}